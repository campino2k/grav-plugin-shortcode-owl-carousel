{% set owl_id = 'owl-' ~ hash %}
<div class="owl-carousel owl-theme" id="{{ owl_id }}">
    {{ owl_items|regex_replace('(^\n?<p>|<\/p>$)','') }}
</div>
{% do assets.addInlineJs(' $(document).ready(function(){ $("#' ~ owl_id ~ '").owlCarousel(' ~ params|json_encode(constant('JSON_NUMERIC_CHECK'))|raw ~ '); }); ') %}
